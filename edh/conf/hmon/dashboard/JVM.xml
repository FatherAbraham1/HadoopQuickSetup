<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dashboardConfig>
    <chartsPane>
        <chartPalette>
            <colors>#008000</colors>
            <colors>#0000FF</colors>
            <colors>#800080</colors>
            <colors>#800000</colors>
            <colors>#00FF00</colors>
            <colors>#FF00FF</colors>
            <colors>#008080</colors>
            <colors>#808080</colors>
            <colors>#00FFFF</colors>
            <colors>#000080</colors>
            <colors>#FF0000</colors>
            <colors>#808000</colors>
        </chartPalette>
        <chartRows>
            <charts>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.memHeapCommittedM</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.memHeapUsedM</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.memNonHeapCommittedM</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.memNonHeapUsedM</metricName>
                </timeSeries>
                <title>Memory</title>
            </charts>
            <charts>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.threadsBlocked</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.threadsNew</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.threadsRunnable</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.threadsTerminated</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.threadsTimedWaiting</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.threadsWaiting</metricName>
                </timeSeries>
                <title>Threads</title>
            </charts>
        </chartRows>
        <chartRows>
            <charts>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <color>#FF0000</color>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.logFatal</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <color>#800000</color>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.logError</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <color>#808000</color>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.logWarn</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <color>#0000FF</color>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.logInfo</metricName>
                </timeSeries>
                <title>Logging</title>
            </charts>
            <charts>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.gcCount</metricName>
                </timeSeries>
                <title>Garbage collection</title>
            </charts>
        </chartRows>
    </chartsPane>
    <heatMapPane>
        <metricName>jvm.metrics.memHeapUsedM</metricName>
    </heatMapPane>
    <systemSummaryPane>
        <metrics>
            <metricName>jvm.metrics.logFatal</metricName>
        </metrics>
        <metrics>
            <metricName>jvm.metrics.logError</metricName>
        </metrics>
        <metrics>
            <metricName>jvm.metrics.memHeapUsedM</metricName>
        </metrics>
    </systemSummaryPane>
</dashboardConfig>
