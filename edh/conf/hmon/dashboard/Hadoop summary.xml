<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<dashboardConfig>
    <chartsPane>
        <chartPalette>
            <colors>#008000</colors>
            <colors>#0000FF</colors>
            <colors>#800080</colors>
            <colors>#800000</colors>
            <colors>#00FF00</colors>
            <colors>#FF00FF</colors>
            <colors>#008080</colors>
            <colors>#808080</colors>
            <colors>#00FFFF</colors>
            <colors>#000080</colors>
            <colors>#FF0000</colors>
            <colors>#808000</colors>
        </chartPalette>
        <chartRows>
            <charts>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.memHeapUsedM</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.memNonHeapUsedM</metricName>
                </timeSeries>
                <title>JVM memory</title>
            </charts>
            <charts>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <color>#FF0000</color>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.logFatal</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <color>#800000</color>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.logError</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Stacked</chartType>
                    <color>#808000</color>
                    <enabled>true</enabled>
                    <metricName>jvm.metrics.logWarn</metricName>
                </timeSeries>
                <title>Logs</title>
            </charts>
            <charts>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>mapred.jobtracker.jobs_running</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>mapred.jobtracker.jobs_failed</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>mapred.jobtracker.jobs_completed</metricName>
                </timeSeries>
                <title>Jobs</title>
            </charts>
        </chartRows>
        <chartRows>
            <charts>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>dfs.datanode.bytes_read</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>dfs.datanode.bytes_written</metricName>
                </timeSeries>
                <title>DFS I/O</title>
            </charts>
            <charts>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>dfs.FSNamesystem.CapacityUsedGB</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>dfs.FSNamesystem.CapacityRemainingGB</metricName>
                </timeSeries>
                <title>DFS Capacity</title>
            </charts>
            <charts>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>rpcdetailed.rpcdetailed.complete_avg_time</metricName>
                </timeSeries>
                <timeSeries>
                    <chartType>Lines</chartType>
                    <enabled>true</enabled>
                    <metricName>rpcdetailed.rpcdetailed.heartbeat_avg_time</metricName>
                </timeSeries>
                <title>RPC</title>
            </charts>
        </chartRows>
    </chartsPane>
    <heatMapPane>
        <metricName>mapred.jobtracker.jobs_running</metricName>
    </heatMapPane>
    <systemSummaryPane>
        <metrics>
            <metricName>mapred.jobtracker.jobs_running</metricName>
        </metrics>
        <metrics>
            <metricName>mapred.jobtracker.jobs_failed</metricName>
        </metrics>
        <metrics>
            <metricName>jvm.metrics.memHeapUsedM</metricName>
        </metrics>
    </systemSummaryPane>
</dashboardConfig>
